{
    "name": "ketchalegend/filament-survey",
    "description": "Filament Laravel Survey plugin with multi tenancy",
    "keywords": [
        "ketchalegend",
        "filament",
        "laravel",
        "survey"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ketchalegend ",
            "email": "kbepa@yahoo.com",
            "role": "SW Developer"
        }
    ],
    "homepage": "https://github.com/Ketchalegend/filament-survey",
    "support": {
        "issues": "https://github.com/Ketchalegend/filament-survey/issues",
        "source": "https://github.com/Ketchalegend/filament-survey"
    },
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0-stable",
        "filament/spatie-laravel-translatable-plugin": "^3.0-stable",
        "maatwebsite/excel": "^3.1",
        "spatie/eloquent-sortable": "^4.0",
        "spatie/laravel-package-tools": "^1.9"
    },
    "require-dev": {
        "doctrine/dbal": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Ketchalegend\\FilamentSurvey\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ketchalegend\\FilamentSurvey\\FilamentSurveyServiceProvider"
            ]
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
